{
  "name": "@etd-framework/js",
  "version": "2.0.0",
  "description": "ETD Framework JS Package",
  "homepage": "https://github.com/etd-framework/js",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/etd-framework/js.git"
  },
  "babel": {
    "comments": false,
    "presets": [
      "es2015",
      "babili"
