<?xml version="1.0" encoding="UTF-8"?>
<TaskOptions>
  <TaskOptions>
    <option name="arguments" value="--no-comments --out-file $ProjectFileDir$/dist/$FileNameWithoutExtension$.js $FilePath$" />
    <option name="checkSyntaxErrors" value="true" />
    <option name="description" value="Transpiles ECMAScript 6 code to ECMAScript 5" />
    <option name="exitCodeBehavior" value="ERROR" />
    <option name="fileExtension" value="es6" />
    <option name="immediateSync" value="true" />
    <option name="name" value="Babel" />
    <option name="output" value="$ProjectFileDir$/dist/$FileNameWithoutExtension$.js" />
    <option name="outputFilters">
      <array />
    </option>
    <option name="outputFromStdout" value="false" />
    <option name="program" value="$ProjectFileDir$/node_modules/.bin/babel" />
    <option name="scopeName" value="Project Files" />
    <option name="trackOnlyRoot" value="true" />
    <option name="workingDir" value="$FileDir$" />
    <envs />
  </TaskOptions>
</TaskOptions>

